{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "required" : [
      "echo_req",
      "msg_type"
   ],
   "type" : "object",
   "description" : null,
   
   "title" : "Website Status Response",
   "properties" : {
      "msg_type" : {
         "description" : "website_status",
         "type" : "string"
      },
      "website_status" : {
         "properties" : {
            "supported_languages" : {
               "description" : "Provides codes for languages supported",
               "type" : "array",
               "items" : {
                  "type" : "string"
               }
            },
            "api_call_limits" : {
               "title" : "API call limits",
               "description" : "Limits for api call for each websocket connection",
               "type" : "object"
            },
            "currencies_config": {
                "type": "object",
                "required": "1",
                "properties": {
                    "^[A-Z]{3-4}$": {
                        "type": "object",
                        "properties": {
                            "fractional_digits": {
                                "type": "number",
                                "pattern": "^[0-9]+$",
                                "required": "1"
                            },
                            "type": {
                                "enum": ["fiat", "crypto"],
                                "required": "1"
                            },
                            "stake_default": {
                                "type": "number",
                                "minimum": "0",
                                "required": "1"
                            },
                            "transfer_between_accounts": {
                                "type": "object",
                                "required": "1",
                                "properties": {
                                    "limits": {
                                        "type": "object",
                                        "required": "1",
                                        "properties": {
                                            "min": {
                                                "type": "number",
                                                "minimum": 0,
                                                "required": "1"
                                            }
                                        }
                                    },
                                    "fees": {
                                        "type": "object",
                                        "required": "1",
                                        "properties": {
                                            "^[A-Z]{3-4}$": {
                                                "type": "number",
                                                "minimum": 0,
                                                "maximum": 7
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "api_call_limits": {
                "type": "object",
                "required": "1",
                "properties": {
                    "max_proposal_subscription": {
                        "type": "object",
                        "required": "1",
                        "properties": {
                            "max": {
                                "type": "number",
                                "pattern": "^5$",
                                "required": "1"
                            },
                            "applies_to": {
                                "type": "string",
                                "required": "1"
                            }
                        }
                    },
                    "max_requests_pricing": {
                        "type": "object",
                        "required": "1",
                        "properties": {
                            "minutely": {
                                "type": "number",
                                "pattern": "^66$",
                                "required": "1"
                            },
                            "hourly": {
                                "type": "number",
                                "pattern": "^266$",
                                "required": "1"
                            },
                            "applies_to": {
                                "type": "string",
                                "required": "1"
                            }
                        }
                    },
                    "max_requests_outcome": {
                        "type": "object",
                        "required": "1",
                        "properties": {
                            "minutely": {
                                "type": "number",
                                "pattern": "^55$",
                                "required": "1"
                            },
                            "hourly": {
                                "type": "number",
                                "pattern": "^255$",
                                "required": "1"
                            },
                            "applies_to": {
                                "type": "string",
                                "required": "1"
                            }
                        }
                    },
                    "max_requestes_general": {
                        "type": "object",
                        "required": "1",
                        "properties": {
                            "minutely": {
                                "type": "number",
                                "pattern": "^244$",
                                "required": "1"
                            },
                            "hourly": {
                                "type": "number",
                                "pattern": "^244$",
                                "required": "1"
                            },
                            "applies_to": {
                                "type": "string",
                                "required": "1"
                            }
                        }
                    }
                }
            },
            "message" : {
               "description" : "Text for site status banner, contains problem description.  Shown only if set by the system",
               "type" : "string"
            },
            "site_status" : {
               "pattern" : "^(up|down)$",
               "description" : "Current status of the site. up or down",
               "type" : "string"
            },
            "clients_country" : {
               "type" : "string",
               "description" : "Country code of connected IP"
            },
            "terms_conditions_version" : {
               "description" : "Latest terms and conditions version",
               "type" : "string"
            }
         },
         "type" : "object"
      },
      "req_id" : {
         "description" : "Optional field send in request to map to response, present only when request contains req_id",
         "type" : "integer"
      },
      "echo_req" : {
         "description" : "Echo of the request made",
         "title" : "Echo request"
      }
   }
}
