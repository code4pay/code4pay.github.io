{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "title" : "Payment Agent Transfer Request",
   "additionalProperties" : false,
   "description" : "Payment Agent Transfer - this call is available only to accounts that are approved Payment Agents.",
   "type" : "object",
   "required" : [
      "amount",
      "currency",
      "transfer_to",
      "paymentagent_transfer"
   ],
   "properties" : {
      "passthrough" : {
         "description" : "Optional field, used to pass data through the websocket, which may be retrieved via the echo_req output field.",
         "type" : "object"
      },
      "currency" : {
         "pattern" : "^[A-Z]{3}$",
         "type" : "string"
      },
      "dry_run" : {
         "description" : "If 1, just do validation",
         "type" : "integer",
         "enum" : [
            0,
            1
         ]
      },
      "paymentagent_transfer" : {
         "type" : "integer",
         "enum" : [
            1
         ]
      },
      "amount" : {
         "type" : "number"
      },
      "transfer_to" : {
         "pattern" : "^\\w+\\d+$",
         "description" : "The transfer_to loginid",
         "type" : "string"
      },
      "req_id" : {
         "type" : "integer",
         "description" : "Optional field to map request to response"
      }
   }
}
